<div class="row">
  <div class="large-centered columns">
    <h1>Welcome to the Home Page </h1>
  </div>
</div>
  <div class="row">
    <div class="large-4 columns">
        <div id="user_nav">
          <% if user_signed_in? %>
                  Signed in as <%= current_user.email %>. Not You?
                    <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
           <% else %>
              <ul>
                <li><%= link_to "Sign In", user_session_path, class: "round  button" %></li>
                <li><%= link_to "Sign up", new_user_registration_path, class: "round  button" %></li>
              </ul>
           <% end %>
        </div>
    </div>
    <div class="large-8 columns">
      <%= image_tag "home_img.jpg" %>
    </div>
  </div>
